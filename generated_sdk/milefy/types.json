        "type": "obj",
        "req": [
            "code",
            "value"
        ],
        "props": {
            "code": {
                "type": "int",
                "enum": [
                    1,
                    2,
                    3,
                    4,
                    5
                ]
            },
            "value": {
                "type": "num",
                "format": "float"
            }
        }
    },
