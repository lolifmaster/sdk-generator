        "type": "obj",
        "req": [
            "program"
        ],
        "props": {
            "program": {
                "type": "str"
            },
            "memberNumber": {
                "type": "str"
            },
            "memberName": {
                "type": "str"
            },
            "mileageEarnings": {
                "type": "array",
                "minItems": 1,
                "uniqueItems": true,
                "items": "Miles"
            },
            "statusTier": {
                "type": "int",
                "enum": [
                    0,
                    1,
                    8,
                    16,
                    24,
                    32,
                    33,
                    34,
                    35,
                    36
                ]
            }
        }
    }
}