{
    "authenticate-payload": {
        "title": "ApiKey",
        "req": [
            "apiKey"
        ],
        "type": "obj",
        "props": {
            "apiKey": {
                "type": "str"
            }
        }
    },
    "refresh-token-payload": {
        "title": "RefreshToken",
        "req": [
            "refreshToken"
        ],
        "type": "obj",
        "props": {
            "refreshToken": {
                "type": "str"
            }
        }
    },
    "current-page": {
        "name": "currentPage",
        "in": "query",
        "req": false,
        "style": "form",
        "explode": true,
        "schema": {
            "type": "int",
            "def": 1
        }
    },
    "page-size": {
        "name": "pageSize",
        "in": "query",
        "req": false,
        "style": "form",
        "explode": true,
        "schema": {
            "type": "int",
            "def": 25
        }
    },
    "metadata": {
        "title": "Metadata",
        "type": "obj",
        "props": {
            "key": {
                "type": "str"
            },
            "value": {
                "type": "str"
            }
        },
        "x-client-all-args-constructor": true
    },
    "video-clip": {
        "type": "obj",
        "props": {
            "startTimecode": {
                "type": "str",
                "pattern": "^(?:\\d{2,3}:[0-5]\\d:[0-5]\\d(?:\\.\\d{1,3}|\\:\\d{1,2})?|\\d{1,7})$"
            },
            "endTimecode": {
                "type": "str",
                "pattern": "^(?:\\d{2,3}:[0-5]\\d:[0-5]\\d(?:\\.\\d{1,3}|\\:\\d{1,2})?|\\d{1,7})$"
            }
        }
    },
    "video-watermark": {
        "type": "obj",
        "props": {
            "id": {
                "type": "str"
            },
            "top": {
                "type": "str"
            },
            "left": {
                "type": "str"
            },
            "bottom": {
                "type": "str"
            },
            "right": {
                "type": "str"
            },
            "width": {
                "type": "str"
            },
            "height": {
                "type": "str"
            },
            "opacity": {
                "type": "str"
            }
        }
    },
    "video-creation-payload": {
        "title": "VideoCreationPayload",
        "type": "obj",
        "props": {
            "tags": {
                "type": "array",
                "items": {
                    "type": "str"
                }
            },
            "title": {
                "type": "str"
            },
            "source": {
                "type": "str"
            },
            "public": {
                "type": "bool",
                "def": true
            },
            "panoramic": {
                "type": "bool",
                "def": false
            },
            "mp4Support": {
                "type": "bool",
                "def": true
            },
            "playerId": {
                "type": "str"
            },
            "metadata": {
                "type": "array",
                "items": {
                    "$ref": "metadata"
                }
            },
            "clip": {
                "$ref": "video-clip"
            },
            "watermark": {
                "$ref": "video-watermark"
            }
        },
        "req": [
            "title"
        ]
    },
    "video-thumbnail-pick-payload": {
        "title": "ThumbnailPickPayload",
        "req": [
            "timecode"
        ],
        "type": "obj",
        "props": {
            "timecode": {
                "pattern": "\\d{2}:\\d{2}:\\d{2}(\\.\\d{2})?",
                "type": "str"
            }
        }
    },
    "video-update-payload": {
        "title": "VideoUpdatePayload",
        "type": "obj",
        "props": {
            "tags": {
                "type": "array",
                "items": {
                    "type": "str"
                }
            },
            "title": {
                "type": "str"
            },
            "playerId": {
                "type": "str",
                "x-optional-nullable": true
            },
            "public": {
                "type": "bool"
            },
            "panoramic": {
                "type": "bool"
            },
            "mp4Support": {
                "type": "bool"
            },
            "metadata": {
                "type": "array",
                "items": "metadata"
            }
        }
    },
    "token-creation-payload": {
        "title": "TokenCreationPayload",
        "type": "obj",
        "props": {
            "ttl": {
                "max": 2147483647,
                "min": 0,
                "type": "int",
                "def": 0
            }
